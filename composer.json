{
  "name": "openexw/aliyun-acm",
  "description": "An aliyun acm adapter",
  "keywords": [
    "php",
    "aliyun",
    "acm"
  ],
  "require": {
    "guzzlehttp/guzzle": "~6.5.5",
    "pimple/pimple": "^3.2.3",
    "doctrine/collections": "^1.6.7",
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5.20",
    "mockery/mockery": "^1.3.4",
    "friendsofphp/php-cs-fixer": "^2.19"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "AliyunAcm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AliyunAcm\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix $1",
    "test": "phpunit --colors=always"
  }
}
